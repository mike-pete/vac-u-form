var s, win, protocol; try{win=window.top;protocol = win.location.protocol;}catch(err){win=window;protocol = win.location.protocol;}if(protocol!='http:'){s=1;}else{s=0;}var hs = win.location.host; var pg = win.location.pathname; var l = navigator.language;var dW = win.screen.width; var dH = win.screen.height;var src = "https://ads40.krushmedia.com/?scriptHash=8e89b2fb18969ae6e7c86418d1ffceef&c=b&m=b&placementId=87&secure="+s+"&host="+hs+"&page="+pg+"&language="+l+"&w="+dW+"&h="+dH;document.write("<script  src='"+src+"'></script>");